<p id="notice"><%= notice %></p>

<h1>Type Requests</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Request</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_requests.each do |type_request| %>
      <tr>
        <td><%= type_request.nombre %></td>
        <td><%= type_request.request %></td>
        <td><%= link_to 'Show', type_request %></td>
        <td><%= link_to 'Edit', edit_type_request_path(type_request) %></td>
        <td><%= link_to 'Destroy', type_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Request', new_type_request_path %>
